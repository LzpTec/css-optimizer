{
    "name": "lastzero/css-optimizer",
    "type": "library",
    "description": "CSS Optimizer - Minifies stylesheets by removing duplicates and combining selectors",
    "keywords": ["css","optimization","minification","assetic","symfony"],
    "homepage": "https://github.com/lastzero/css-optimizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Mayer",
            "email": "michael@lastzero.net",
            "homepage": "http://lastzero.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "natxet/CssMin": "dev-master",
        "kriswallsmith/assetic": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "lastzero/test-tools": "*",
        "sstalle/php7cc": "*"
    },
    "autoload": {
        "psr-0": { "CssOptimizer\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
